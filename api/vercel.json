{
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    },
    "api/*.mjs": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    { "source": "/api/contact", "destination": "/api/contact.mjs" },
    { "source": "/api/booking", "destination": "/api/booking.js" },
    { "source": "/api/banquet-booking", "destination": "/api/banquet-booking.js" }
  ]
}
